        <xsd:attribute type="xsd:string" name="guid"/>
        <xsd:attribute type="xsd:string" name="code" use="required"/>
        <xsd:attribute type="entityImportModeType" name="import-mode"/>
    </xsd:complexType>

    <xsd:complexType name="fulfilment-centre-configurationType">
        <xsd:sequence>
            <xsd:element type="xsd:int" name="standard-stock-lead-time"/>
            <xsd:element type="xsd:int" name="backorder-stock-lead-time"/>
            <xsd:element type="xsd:boolean" name="multiple-shipping-supported"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="fulfilment-centre-locationType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="iso-3166-1-alpha2" minOccurs="0"/>
            <xsd:element type="xsd:string" name="region-code" minOccurs="0"/>
            <xsd:element type="xsd:string" name="city" minOccurs="0"/>
            <xsd:element type="xsd:string" name="postcode" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>



    <!-- Shipping -->

    <xsd:element name="shipping-providers" type="shipping-providersType"/>
    <xsd:element name="shipping-provider" type="shipping-providerType"/>

    <xsd:complexType name="shipping-providersType">
        <xsd:sequence>
            <xsd:element type="shipping-providerType" name="shipping-provider" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="shipping-providerType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="name"/>
            <xsd:element type="i18nsType" name="display-name" minOccurs="0"/>
            <xsd:element type="xsd:string" name="description" minOccurs="0"/>
            <xsd:element type="i18nsType" name="display-description" minOccurs="0"/>
            <xsd:element type="shipping-provider-configurationType" name="configuration" minOccurs="0"/>
            <xsd:element type="shipping-provider-shipping-methodsType" name="shipping-methods" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="created-timestamp" minOccurs="0"/>
            <xsd:element type="xsd:string" name="created-by" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="updated-timestamp" minOccurs="0"/>
            <xsd:element type="xsd:string" name="updated-by" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute type="xsd:long" name="id"/>
        <xsd:attribute type="xsd:string" name="guid" use="required"/>
        <xsd:attribute type="entityImportModeType" name="import-mode"/>
    </xsd:complexType>

    <xsd:complexType name="shipping-provider-configurationType">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:boolean" name="worldwide"/>
                <xsd:attribute type="xsd:boolean" name="country"/>
                <xsd:attribute type="xsd:boolean" name="state"/>
                <xsd:attribute type="xsd:boolean" name="local"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="shipping-provider-shipping-methodsType">
        <xsd:sequence>
            <xsd:element type="shipping-methodType" name="shipping-method" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute type="collectionImportModeType" name="import-mode"/>
    </xsd:complexType>


    <xsd:element name="shipping-methods" type="shipping-methodsType"/>
    <xsd:element name="shipping-method" type="shipping-methodType"/>

    <xsd:complexType name="shipping-methodsType">
        <xsd:sequence>
            <xsd:element type="shipping-methodType" name="shipping-method" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="shipping-methodType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="name"/>
            <xsd:element type="i18nsType" name="display-name" minOccurs="0"/>
            <xsd:element type="xsd:string" name="description" minOccurs="0"/>
            <xsd:element type="i18nsType" name="display-description" minOccurs="0"/>
            <xsd:element type="shipping-method-configurationType" name="configuration" minOccurs="0"/>
            <xsd:element type="shipping-method-exclusionsType" name="exclusions" minOccurs="0"/>
            <xsd:element type="shipping-method-supportedType" name="supported" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="created-timestamp" minOccurs="0"/>
            <xsd:element type="xsd:string" name="created-by" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="updated-timestamp" minOccurs="0"/>
            <xsd:element type="xsd:string" name="updated-by" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute type="xsd:long" name="id"/>
        <xsd:attribute type="xsd:string" name="guid" use="required"/>
        <xsd:attribute type="xsd:string" name="provider"/>
        <xsd:attribute type="xsd:string" name="external-ref"/>
        <xsd:attribute type="entityImportModeType" name="import-mode"/>
    </xsd:complexType>

    <xsd:complexType name="shipping-method-configurationType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="script" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute type="xsd:string" name="type" use="required"/>
        <xsd:attribute type="xsd:int" name="min-days"/>
        <xsd:attribute type="xsd:int" name="max-days"/>
        <xsd:attribute type="xsd:boolean" name="guaranteed-delivery" use="required"/>
        <xsd:attribute type="xsd:boolean" name="named-day-delivery" use="required"/>
        <xsd:attribute type="xsd:boolean" name="billing-address-not-required" use="required"/>
        <xsd:attribute type="xsd:boolean" name="delivery-address-not-required" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="shipping-method-exclusionsType">
        <xsd:sequence>
            <xsd:element type="shipping-method-exclusions-customer-typesType" name="customer-types" minOccurs="0"/>
            <xsd:element type="shipping-method-exclusions-weekdaysType" name="weekdays" minOccurs="0"/>
            <xsd:element type="shipping-method-exclusions-datesType" name="dates" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="shipping-method-exclusions-customer-typesType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="type" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="shipping-method-exclusions-weekdaysType">
        <xsd:sequence>
            <xsd:element name="weekday" maxOccurs="unbounded">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="MONDAY"/>
                        <xsd:enumeration value="TUESDAY"/>
                        <xsd:enumeration value="WEDNESDAY"/>
                        <xsd:enumeration value="THURSDAY"/>
                        <xsd:enumeration value="FRIDAY"/>
                        <xsd:enumeration value="SATURDAY"/>
                        <xsd:enumeration value="SUNDAY"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="shipping-method-exclusions-datesType">
        <xsd:sequence>
            <xsd:element type="shipping-method-exclusions-dates-dateType" name="date" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="shipping-method-exclusions-dates-dateType">
        <xsd:sequence>
            <xsd:element type="dateType" name="from" minOccurs="0"/>
            <xsd:element type="dateType" name="to" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="shipping-method-supportedType">
        <xsd:sequence>
            <xsd:element type="shipping-method-supported-fulfilment-centresType" name="fulfilment-centres" minOccurs="0"/>
            <xsd:element type="shipping-method-supported-payment-gatewaysType" name="payment-gateways" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="shipping-method-supported-fulfilment-centresType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="fulfilment-centre" maxOccurs="unbounded" minOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="shipping-method-supported-payment-gatewaysType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="payment-gateway" maxOccurs="unbounded" minOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- Promotions -->

    <xsd:element name="promotions" type="promotionsType"/>
    <xsd:element name="promotion" type="promotionType"/>

    <xsd:complexType name="promotionsType">
        <xsd:sequence>
            <xsd:element type="promotionType" name="promotion" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="promotionType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="name"/>
            <xsd:element type="i18nsType" name="display-name" minOccurs="0"/>
            <xsd:element type="xsd:string" name="description" minOccurs="0"/>
            <xsd:element type="i18nsType" name="display-description" minOccurs="0"/>
            <xsd:element type="xsd:string" name="tag" minOccurs="0"/>
            <xsd:element name="availability">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element type="dateTimeType" name="available-from" minOccurs="0"/>
                        <xsd:element type="dateTimeType" name="available-to" minOccurs="0"/>
                    </xsd:sequence>
                    <xsd:attribute type="xsd:boolean" name="disabled" use="required"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element type="promotion-configurationType" name="configuration"/>
            <xsd:element type="promotion-coupons-couponsType" name="coupons" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="created-timestamp" minOccurs="0"/>
            <xsd:element type="xsd:string" name="created-by" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="updated-timestamp" minOccurs="0"/>
            <xsd:element type="xsd:string" name="updated-by" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute type="xsd:long" name="id"/>
        <xsd:attribute type="xsd:string" name="guid"/>
        <xsd:attribute type="xsd:string" name="code" use="required"/>
        <xsd:attribute type="xsd:string" name="shop" use="required"/>
        <xsd:attribute type="xsd:string" name="currency" use="required"/>
        <xsd:attribute type="xsd:int" name="rank"/>
        <xsd:attribute type="entityImportModeType" name="import-mode"/>
    </xsd:complexType>

    <xsd:complexType name="promotion-configurationType">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:string" name="action" use="required"/>
                <xsd:attribute type="xsd:string" name="action-context"/>
                <xsd:attribute type="xsd:string" name="type" use="required"/>
                <xsd:attribute type="xsd:boolean" name="coupon-triggered" use="required"/>
                <xsd:attribute type="xsd:boolean" name="can-be-combined" use="required"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="promotion-coupons-couponsType">
        <xsd:sequence>
            <xsd:element type="promotion-couponType" name="promotion-coupon" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute type="collectionImportModeType"  name="import-mode"/>
    </xsd:complexType>

    <xsd:element name="promotion-coupons" type="promotion-couponsType"/>
    <xsd:element name="promotion-coupon" type="promotion-couponType"/>

    <xsd:complexType name="promotion-couponsType">
        <xsd:sequence>
            <xsd:element type="promotion-couponType" name="promotion-coupon" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="promotion-couponType">
        <xsd:sequence>
            <xsd:element type="promotion-coupon-configurationType" name="configuration" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="created-timestamp" minOccurs="0"/>
            <xsd:element type="xsd:string" name="created-by" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="updated-timestamp" minOccurs="0"/>
            <xsd:element type="xsd:string" name="updated-by" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute type="xsd:long" name="id"/>
        <xsd:attribute type="xsd:string" name="guid"/>
        <xsd:attribute type="xsd:string" name="code" use="required"/>
        <xsd:attribute type="xsd:string" name="promotion"/>
        <xsd:attribute type="xsd:int" name="usage-count" use="optional"/>
        <xsd:attribute type="entityImportModeType" name="import-mode"/>
    </xsd:complexType>

    <xsd:complexType name="promotion-coupon-configurationType">
        <xsd:attribute type="xsd:int" name="max-limit" use="required"/>
        <xsd:attribute type="xsd:int" name="customer-limit" use="required"/>
    </xsd:complexType>

    <!-- Managers -->

    <xsd:element name="organisation-users" type="organisation-usersType"/>
    <xsd:element name="organisation-user" type="organisation-userType"/>

    <xsd:complexType name="organisation-usersType">
        <xsd:sequence>
            <xsd:element type="organisation-userType" name="organisation-user" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="organisation-userType">
        <xsd:sequence>
            <xsd:element type="organisation-userCredentialsType" name="credentials"/>
            <xsd:element type="organisation-userOrganisationType" name="organisation" minOccurs="0"/>
            <xsd:element type="organisation-userContact-detailsType" name="contact-details" minOccurs="0"/>
            <xsd:element type="organisation-userPreferencesType" name="preferences" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="created-timestamp" minOccurs="0"/>
            <xsd:element type="xsd:string" name="created-by" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="updated-timestamp" minOccurs="0"/>
            <xsd:element type="xsd:string" name="updated-by" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute type="xsd:long" name="id"/>
        <xsd:attribute type="xsd:string" name="guid"/>
        <xsd:attribute type="entityImportModeType" name="import-mode"/>
    </xsd:complexType>

    <xsd:complexType name="organisation-userShopsType">
        <xsd:sequence>
            <xsd:element type="organisation-userShopType" name="shop" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute type="collectionImportModeType"  name="import-mode"/>
    </xsd:complexType>

    <xsd:complexType name="organisation-userRolesType">
        <xsd:sequence>
            <xsd:element type="organisation-userRoleType" name="role" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute type="collectionImportModeType"  name="import-mode"/>
    </xsd:complexType>

    <xsd:complexType name="organisation-userPreferencesType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="dashboard-widgets"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="organisation-userCredentialsType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="email"/>
            <xsd:element type="xsd:string" name="password" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="password-expiry" minOccurs="0"/>
            <xsd:element type="xsd:boolean" name="enabled" minOccurs="0"/>
            <xsd:element type="xsd:string" name="auth-token" minOccurs="0"/>
            <xsd:element type="dateTimeType" name="auth-token-expiry" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="organisation-userContact-detailsType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="salutation" minOccurs="0"/>
            <xsd:element type="xsd:string" name="firstname" minOccurs="0"/>
            <xsd:element type="xsd:string" name="middlename" minOccurs="0"/>
            <xsd:element type="xsd:string" name="lastname" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="organisation-userShopType">
        <xsd:attribute type="xsd:string" name="code" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="organisation-userRoleType">
        <xsd:attribute type="xsd:string" name="code" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="organisation-userOrganisationType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="company-name-1" minOccurs="0"/>
            <xsd:element type="xsd:string" name="company-name-2" minOccurs="0"/>
            <xsd:element type="xsd:string" name="company-department" minOccurs="0"/>
            <xsd:element type="organisation-userShopsType" name="shops" minOccurs="0"/>
            <xsd:element type="organisation-userRolesType" name="roles" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>


</xsd:schema>